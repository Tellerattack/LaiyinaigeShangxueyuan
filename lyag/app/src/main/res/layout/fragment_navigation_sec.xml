<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_white"
    android:orientation="vertical">


    <ExpandableListView
        android:id="@+id/expandable_listview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:childDivider="@color/color_white"
        android:divider="@color/color_white"
        android:dividerHeight="@dimen/default_margin_16"
        android:scrollbars="none"
        android:layout_marginLeft="@dimen/default_margin_16"
        android:layout_marginRight="@dimen/default_margin_16"
        android:layout_marginTop="@dimen/default_margin"
        />

</LinearLayout>